<?xml version="1.0" encoding="UTF-8" ?>
<!--
	svs specific typemapping
	
	@author	andre pflueger
	@date	2010-04-13
 -->
<tns:typeMapping xmlns:tns="http://www.genesez.de/typemapping"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tns:include>
		<tns:file>de/genesez/platforms/java/typemapping/typemapping.xml</tns:file>
	</tns:include>
	<tns:externalTypes>
		<!-- we used the type 'Money' and declared it as 'external' within the uml model -->
		<tns:type from="Money">
			<!-- default mapping should return the type name -->
			<tns:to>BigDecimal</tns:to>
			<!-- used to generate imports -->
			<tns:context name="Import">javax.math.BigDecimal</tns:context>
		</tns:type>
	</tns:externalTypes>
</tns:typeMapping>