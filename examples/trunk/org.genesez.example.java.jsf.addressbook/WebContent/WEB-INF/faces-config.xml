<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>personBean</managed-bean-name>
		<managed-bean-class>contacts.jsf.PersonBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>contacts.jsf.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>organisationBean</managed-bean-name>
		<managed-bean-class>contacts.jsf.OrganisationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>addressBook/home.xhtml</display-name>
		<from-view-id>/addressBook/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/addressBook/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/home.xhtml</display-name>
		<from-view-id>/addressBook/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>plist</from-outcome>
			<to-view-id>/addressBook/persList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/home.xhtml</display-name>
		<from-view-id>/addressBook/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>olist</from-outcome>
			<to-view-id>/addressBook/orgaList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/persList.xhtml</display-name>
		<from-view-id>/addressBook/persList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>pedit</from-outcome>
			<to-view-id>/addressBook/persForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/persForm.xhtml</display-name>
		<from-view-id>/addressBook/persForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>plist</from-outcome>
			<to-view-id>/addressBook/persList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/persList.xhtml</display-name>
		<from-view-id>/addressBook/persList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/addressBook/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/orgaList.xhtml</display-name>
		<from-view-id>/addressBook/orgaList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>oedit</from-outcome>
			<to-view-id>/addressBook/orgaForm.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/orgaForm.xhtml</display-name>
		<from-view-id>/addressBook/orgaForm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>olist</from-outcome>
			<to-view-id>/addressBook/orgaList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/orgaList.xhtml</display-name>
		<from-view-id>/addressBook/orgaList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/addressBook/home.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/orgaList.xhtml</display-name>
		<from-view-id>/addressBook/orgaList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>oshow</from-outcome>
			<to-view-id>/addressBook/orgaShow.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>addressBook/orgaShow.xhtml</display-name>
		<from-view-id>/addressBook/orgaShow.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>olist</from-outcome>
			<to-view-id>/addressBook/orgaList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
