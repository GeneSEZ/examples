<tns:typeMapping xmlns:tns="http://www.genesez.de/typemapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:include>
  	<tns:file>de/genesez/platforms/php/typemapping/typemapping.xml</tns:file>
  </tns:include>
  <tns:externalTypes>
  <!-- container is the base class for custom authentication providers from 'PEAR::Auth' package -->
  	<tns:type from="Auth_Container">
	<!-- default mapping is to the class name -->
		<tns:to>Auth_Container</tns:to>
		<!-- used to generate imports -->
		<tns:context name="import">Auth/Container.php</tns:context>
	</tns:type>
  </tns:externalTypes>
</tns:typeMapping>
