module org.genesez.example.documentation.umlmodel2latex

Workflow {
	component = @org.genesez.adapter.uml2.gcore.Uml2Gcore {
		uml2file = "model/greq.uml"
	}
	component = org.genesez.workflow.xpand.Model2TextComposite {
		component = org.genesez.workflow.xpand.Model2Text {
			emfMetaModelPackage = "org.genesez.metamodel.gcore.GcorePackage"
			slot = "coremodel"
			template = "org::genesez::platform::documentation::latex::Root::Root"
			outputDir = "output/latex"
		}
	}
}
