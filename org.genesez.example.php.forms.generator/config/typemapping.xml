<?xml version="1.0" encoding="UTF-8" ?>
<!--
	example for a project specific typemapping
	
	@author	tobias haubold
	@date	2009-08-05
 -->
<tns:typeMapping xmlns:tns="http://www.genesez.org/typemapping"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<tns:include>
		<tns:file>org/genesez/platform/php/typemapping/typemapping.xml</tns:file>
	</tns:include>
	<!-- mapping of our domain types to technical types -->
	<tns:primitiveTypes>
		<tns:type from="Coordinate">
			<tns:to>float</tns:to>
		</tns:type>
		<tns:type from="Angle">
			<tns:to>float</tns:to>
		</tns:type>
		<tns:type from="Distance">
			<tns:to>float</tns:to>
		</tns:type>
	</tns:primitiveTypes>
</tns:typeMapping>
